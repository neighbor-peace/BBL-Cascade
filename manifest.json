{
  "manifest_version": 3,
  "name": "Beautiful Bouncing Lobster Cascade",
  "version": "1.0",
  "description": "Lobster Catching Productivity",
  "action": {
    "default_title": "Beautiful Bouncing Lobster Cascade",
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "js": ["scripts/content.js"],
      "css": ["./scripts/content.css"],
      "matches": ["<all_urls>"]
    }
  ],

  "icons": {
    "128": "./scripts/Lobster2.png"
  }
}
